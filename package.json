{
    "name": "@freshheads/cookie-guard",
    "version": "4.0.0-alpha.1",
    "description": "Let user manage cookie usage",
    "scripts": {
        "dev": "vite",
        "build": "vite build && tsc --noEmit",
        "preview": "vite preview",
        "prepare": "npm run build",
        "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
    },
    "files": [
        "dist"
    ],
    "types": "./dist/index.d.ts",
    "type": "module",
    "main": "./dist/cookie-guard.cjs",
    "module": "./dist/cookie-guard.js",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/freshheads/cookie-guard.git"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "author": "Freshheads",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/freshheads/cookie-guard/issues"
    },
    "devDependencies": {
        "@types/js-cookie": "^2.2.2",
        "@types/lodash": "^4.14.134",
        "@types/react": "^18.0.31",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react": "^3.1.0",
        "path": "^0.12.7",
        "postcss-nesting": "^11.2.2",
        "prettier": "^2.8.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "^4.9.5",
        "vite": "^4.2.0",
        "vite-plugin-dts": "^2.3.0"
    },
    "dependencies": {
        "js-cookie": "^2.2.0",
        "lodash": "^4.17.11"
    },
    "peerDependencies": {
        "react": ">=17",
        "react-dom": ">=17"
    },
    "homepage": "https://github.com/freshheads/cookie-guard#readme",
    "directories": {
        "doc": "doc",
        "lib": "dist"
    }
